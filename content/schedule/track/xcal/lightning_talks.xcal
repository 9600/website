      <pentabarf:title>Wazaabi - Model your UI ... live</pentabarf:title>
      <pentabarf:subtitle>Model your UI ... live</pentabarf:subtitle>
      <pentabarf:language>English</pentabarf:language>
      <pentabarf:language-code>en</pentabarf:language-code>
      <pentabarf:track>Lightning Talks</pentabarf:track>
      <pentabarf:start>2012-02-04 18:40:00 +0100</pentabarf:start>
