      <pentabarf:title>Graphity: an efficient neo4j based graph model for retrieving the top k news feeds for users in social networks </pentabarf:title>
      <pentabarf:subtitle></pentabarf:subtitle>
      <pentabarf:language>English</pentabarf:language>
      <pentabarf:language-code>en</pentabarf:language-code>
      <pentabarf:track>Graph Processing Devroom</pentabarf:track>
      <pentabarf:start>2012-02-05 12:45:00 +0100</pentabarf:start>
